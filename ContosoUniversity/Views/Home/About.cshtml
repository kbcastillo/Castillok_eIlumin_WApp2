@model IEnumerable<ContosoUniversity.ViewModels.EnrollmentDateGroup>
@{
    ViewBag.Title = "About MST Ilumicast®";
}
<h2>Student Body Statistics</h2>
<table>
    <tr>
        <th>
            MST Ilumicast®
        </th>
        <th>
            Mobile Sales Tool
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EnrollmentDate)
            </td>
            <td>
                @item.StudentCount
            </td>
        </tr>
    }
</table>